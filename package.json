  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "dependencies": {
    "framer-motion": "^10.16.4",
    "react-native-dynamic-vector-icons": "^1.3.0",
    "react-native-floating-action-button": "^0.2.2",
    "react-native-material-ripple": "^0.9.1",
    "react-native-vector-icons": "^10.0.0",
    "react-native-version": "^4.0.0"
  }
}
